{
  "name": "botloader-vscode",
  "displayName": "botloader",
  "description": "Botloader integration for vs code",
  "version": "0.0.1",
  "publisher": "botloader",
  "repository": {
    "url": "https://github.com/BotLoader/botloader"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:botloader-vscode.setup-workspace",
    "onCommand:botloader-vscode.set-accesstoken",
    "workspaceContains:/.botloader/index.json"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "botloader-vscode.setup-workspace",
        "title": "Set up workspace...",
        "category": "BotLoader"
      },
      {
        "command": "botloader-vscode.set-accesstoken",
        "title": "Set API key",
        "category": "BotLoader"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "test-compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.61.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@vscode/test-electron": "^1.6.2",
    "esbuild": "^0.13.9",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "node-fetch": "^3.0.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "node-fetch": "^3.0.0"
  }
}